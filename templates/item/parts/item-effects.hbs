{{log item}}
<ol class="effects items-list effects-list">

      <li class="items-header flexrow" data-effect-type="passive" data-parent-id="{{item.actor.id}}" >
        <h3 class="item-name effect-name flexrow">{{localize "EFFECT.Name"}}</h3>
          <div class="effect-source">Effets</div>
          <div class="effect-duration">{{localize "EFFECT.TabDuration"}}</div>
          <div class="item-controls effect-controls flexrow">
            <a class="effect-control" data-action="create" title="{{localize 'DOCUMENT.Create' type="Effect"}}">
              <i class="fas fa-plus"></i> {{localize "DOCUMENT.New" type="Effect"}}
            </a>
          </div>
        </li>

        <ol class="item-list">
          {{#each data.effects as |effect|}}
            <li class="item effect flexrow" data-effect-id="{{effect._id}}" data-parent-id="{{effect.origin}}">
              <div class="item-name effect-name flexrow">
                <h4>{{effect.name}}</h4>
              </div>
              <div class="effect-source">
                  {{#each effect.changes as |change i|}}
                    <ol class="effect-change flexrow" data-index="{{i}}" style="padding: 0;">
                        <span name="system.effect">{{change.value}} {{localize (lookup @root.config.activeEffectChanges change.key)}}</span>
                        <input type="hidden" name="system.effect" value="{{change.value}} {{localize (lookup @root.config.activeEffectChanges change.key)}}"></input>
                    </ol>
                  {{/each}}
              </div>
              <div class="effect-duration">{{effect.duration.rounds}}</div>
              <div class="item-controls effect-controls flexrow">
                <a class="effect-control" data-action="toggle" title="{{localize 'SHINZO.Effect.Toggle'}}">
                  <i class="fas {{#if effect.disabled}}fa-check{{else}}fa-times{{/if}}"></i>
                </a>
                <a class="effect-control" data-action="edit" title="{{localize 'DOCUMENT.Update' type="Effect"}}">
                  <i class="fas fa-edit"></i>
                </a>
                <a class="effect-control" data-action="delete" title="{{localize 'DOCUMENT.Delete' type="Effect"}}">
                  <i class="fas fa-trash"></i>
                </a>
              </div>
            </li>
          {{/each}}
        </ol>
</ol>